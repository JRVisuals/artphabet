{
  "extends": ["tslint:latest", "tslint-react" ], //, "tslint-config-prettier"
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false
  },

  "jsRules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false
  }
}
